{
  "name": "chat",
  "version": "1.0.0",
  "description": "chat",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/icon.png --prune=true --out=release-builds"
  },
  "author": "Evan",
  "license": "ISC",
  "devDependencies": {},
  "dependencies": {
    "child_process": "^1.0.2",
    "electron": "^4.0.8",
    "electron-packager": "^13.1.1",
    "electron-prompt": "^1.3.0",
    "fix-path": "^2.1.0"
  }
}
